	pages = {726--736},
	file = {Full Text PDF:/Users/mariak/Zotero/storage/RY5MBB7G/Volkova und Bachrach - 2015 - On Predicting Sociodemographic Traits and Emotions.pdf:application/pdf;Snapshot:/Users/mariak/Zotero/storage/P5XHNLN6/cyber.2014.html:text/html}
}

